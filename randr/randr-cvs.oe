SECTION="libs"
MAINTAINER="Greg Gilbert <greg@treke.net>"
PROVIDES=randr

SRC_URI =   cvs://anoncvs:anoncvs@pdx.freedesktop.org/cvs/xlibs;module=Randr
S=${WORKDIR}/Randr

inherit autotools pkgconfig

do_stage() {
	oe_runmake install DESTDIR=${STAGING_DIR} \
	       bindir=/build/bin \
	       includedir=/target/include \
	       libdir=/target/lib \
	       datadir=/share
	       prefix=
}
